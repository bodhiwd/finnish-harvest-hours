{
  "name": "finnish-harvest-hours",
  "version": "0.0.1",
  "engines": {
    "node": "9"
  },
  "description": "Compare your total logged hours in Harvest with standard Finnish working hours",
  "main": "index.js",
  "scripts": {
    "server":"pulp build && cp -r src/server/* output/Server && cp .env output && pulp --watch run -m Server",
    "start": "npm run bundle:css && node src/server/server.js",
    "start:watch": "yarn watch:server & yarn watch:less",
    "watch:server": "nodemon --watch 'server/**/*.js' src/server/server.js",
    "watch:less": "chokidar --initial 'styles' -c 'yarn bundle:css'",
    "bundle:css": "lessc styles/main.less server/static/styles/main.css",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaalto/finnish-harvest-hours.git"
  },
  "author": "Joonas Aalto",
  "license": "MIT",
  "homepage": "https://github.com/joaalto/finnish-harvest-hours",
  "dependencies": {
    "body-parser": "^1.14.2",
    "browserify": "^16.1.0",
    "connect-mongo": "^1.1.0",
    "express": "^4.16.2",
    "express-session": "^1.15.0",
    "finnish-holidays-js": "^1.1.0",
    "less": "^2.7.2",
    "lodash": "^4.4.0",
    "mongoose": "^4.6.2",
    "passport": "^0.3.2",
    "passport-oauth2": "^1.1.2",
    "promise": "^7.1.1",
    "superagent": "^1.7.2",
    "superagent-promise": "^1.1.0",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "elm-test": "^0.18.2",
    "nodemon": "^1.11.0"
  }
}
